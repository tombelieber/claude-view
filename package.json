{
  "name": "claude-view",
  "version": "0.1.0",
  "description": "Browse and export Claude Code sessions as beautiful HTML",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "concurrently \"cargo run -p vibe-recall-server\" \"vite --port 5173\"",
    "dev:client": "vite --port 5173",
    "build": "vite build",
    "build:client": "vite build",
    "preview": "cargo run -p vibe-recall-server",
    "start": "cargo run -p vibe-recall-server --release",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "cargo test --workspace"
  },
  "keywords": [
    "claude",
    "anthropic",
    "viewer",
    "export",
    "conversation",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tanstack/react-query": "^5.90.18",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "prism-react-renderer": "^2.3.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^7.13.0",
    "react-virtuoso": "^4.18.1",
    "remark-gfm": "^4.0.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.5",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^22.0.0",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.1.2",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^17.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
